        <record id="ir_cron_whatsapp_reminder_48h" model="ir.cron">
            <field name="name">CUTAI: Recordatorios WhatsApp 48 horas</field>
            <field name="model_id" ref="calendar.model_calendar_event"/>
            <field name="state">code</field>
            <field name="code">model.search([
    ('start', '&gt;=', (datetime.datetime.now() + datetime.timedelta(hours=47)).strftime('%Y-%m-%d %H:%M:%S')),
    ('start', '&lt;=', (datetime.datetime.now() + datetime.timedelta(hours=49)).strftime('%Y-%m-%d %H:%M:%S')),
    ('whatsapp_reminder_48h_sent', '=', False),
    ('appointment_state', 'in', ['scheduled', 'confirmed']),
]).send_whatsapp_notification('reminder_48h')</field>
            <field name="interval_type">hours</field>
            <field name="interval_number">6</field>
            <field name="active" eval="True"/>
        </record>

